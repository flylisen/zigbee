<!--pages/scenesctr/scenesctr.wxml-->
        <view class="link">
        <block  wx:for="{{scenes}}" wx:for-item="item">
        <view bindtap='chang' data-id="{{item}}" class="link_child" id="{{index}}">
               <image src="{{index==ins?'/images/scenesctr/scenesettringdj.png':'/images/scenesctr/scenesettringwdj.png'}}" mode="scaleToFill" class="background-image"></image>
               <image class='btnImg1' src='../../images/scenesctr/scene.png' style="width:40px;height:40px;"></image>
                <view class='btnImg2' wx:if="{{item.siName==''}}">场景</view>
                <view class='btnImg2' wx:else>{{item.siName}}</view>
         </view>
       </block>
        </view>


