     <h1>全部设备</h1>
     <button class='btn' bindtap='bindAdd'>配置</button>
     </view>
     <view class='viewAN'>
         <view class="list_l" bindtap='jumpPagePY1'>
        <image src="../../images/devctr/shebei.png" bindload="imageLoad" mode='aspectFit'></image>
        <text>设备1</text>
        </view>
         <view class="list_l" bindtap='jumpPagePY2'>
        <image src="../../images/devctr/shebei.png" bindload="imageLoad" mode='aspectFit'></image>
