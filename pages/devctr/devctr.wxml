        <text>设备3</text>
        </view>
     </view>
       <view class='viewAN'>
         <view class="list_l" bindtap='jumpPagePY4'>
        <image src="../../images/devctr/shebei.png" bindload="imageLoad" mode='aspectFit'></image>
        <text>设备3</text>
        </view>
         <view class="list_l" bindtap='jumpPagePY5'>
        <image src="../../images/devctr/shebei.png" bindload="imageLoad" mode='aspectFit'></image>
        <text>设备4</text>
        </view>
         <view class="list_l" bindtap='jumpPageP6'>
        <image src="../../images/devctr/shebei.png" bindload="imageLoad" mode='aspectFit'></image>
        <text>设备5</text>
        </view>
     </view>
      <!--模态框-->
     <view class="mask" catchtouchmove="preventTouchMove" wx:if="{{showModal}}"></view>
      <view class='modalDlg' wx:if="{{showModal}}">
        <button bindtap="go">关闭</button>
     </view>
</view>