<!--pages/areaconfig/areaconfig.wxml--> 
<view class='viewh1'> 
     <h1>区域配置列表</h1>  
     <button class='btn' bindtap='bindAdd'>新增</button> 
     </view>
      <view class='flex-wrp_one'>
   <block  wx:for="{{sortedAreas}}" wx:for-item="item">
       <view>
               <image src='../../images/areactr/{{item.atImg}}' data-id="{{item}}"    bindtap='areainfo' style="width:70px;height:70px;"></image>
                <view wx:if="{{item.aiName==''}}">区域</view>
                <view wx:else>{{item.aiName}}</view>
      </view>
    </block>
    </view> 
