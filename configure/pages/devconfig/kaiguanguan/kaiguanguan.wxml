<view class='imagesize2'>
           <view wx:if="{{chuanglians>0}}">
               <text>状态: </text>
               <text style='color:#33ff33;'> 正常</text>
           </view>
            <view wx:else>
              <text>状态: </text>
              <text style='color:red;'> 异常</text>         
           </view>
</view>
<view>
    <form bindsubmit='submit'>
     <view class='bd'>
     <text class='text'>展示名称</text>
     <input type="text" class="input" value="{{diNames}}" name="showname" maxlength='8'></input>
     </view>
      <view class='bd'>
      <text class='text'>内存名称</text>
      <input type="text" class="input" value="{{diName}}"  name="name" maxlength='10'></input>
      </view>
      <button class='btn' form-type='submit' hover-class='btn-hover'>确定</button>
     </form>
      <button bindtap='qingjingsc' class='btn2' hover-class='btn2-hover'>删除</button>
</view>
