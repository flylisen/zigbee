<view class='imagesize1'>
    <view wx:if="{{diNames==''}}">
                    <view wx:if="{{diDeviceid==1026&&diZonetype==42}}"><text>水浸传感器</text></view>
                    <view wx:elif="{{diDeviceid==1026&&diZonetype==44}}"><text>紧急按钮</text></view>
                    <view wx:elif="{{diDeviceid==1026&&diZonetype==277}}"><text>安防遥控器</text></view>
                    <view wx:elif="{{diDeviceid==1026&&diZonetype==13}}"><text>人体传感器</text></view>
                    <view wx:elif="{{diDeviceid==1026&&diZonetype==32769}}"><text>一氧化碳传感器</text></view>
                    <view wx:elif="{{diDeviceid==1026&&diZonetype==21}}"><text>门磁传感器</text></view>
