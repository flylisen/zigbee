                    <view wx:elif="{{diDeviceid==83}}"><text>水表</text></view>
                    <view wx:elif="{{diDeviceid==10}}"><text>门锁</text></view>
                    <view wx:elif="{{diDeviceid==1027&&diZonetype==549}}"><text>声光报警器</text></view>
                     <view wx:elif="{{diDeviceid==1026&&diZonetype==43}}"><text>可燃气体传感器</text></view>
                    <view wx:elif="{{diDeviceid==771}}"><text>新风面板</text></view>
                    <view wx:elif="{{diDeviceid==9}}"><text>插座</text></view>
